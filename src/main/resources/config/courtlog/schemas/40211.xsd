<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE schema [
	<!ENTITY defaultEvent SYSTEM "defaultEvent.xml">
]>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<!--Quash Count-->
	<xs:include schemaLocation="basic_elements.xsd"/>
	<xs:element name="event">
		<xs:complexType>
			<xs:all>
				&defaultEvent;
				<xs:element ref="E40211_Quash_Options"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="E40211_Quash_Options">
		<xs:complexType>
			<xs:all>
					 <xs:element ref="E40211_Quash_Count_Number"/>
                <xs:element ref="E40211_Quash_Indictment_Number"/>
                <xs:element ref="E40211_Quash_Case_Number"/>
			</xs:all>
		</xs:complexType>
	</xs:element>
	<xs:element name="E40211_Quash_Count_Number" type="xs:string"/>
	<xs:element name="E40211_Quash_Indictment_Number" type="xs:string"/>
	<xs:element name="E40211_Quash_Case_Number" type="xs:string"/>
</xs:schema>
